.page__hd {
  height:100vh;
  padding: 20rpx 35rpx 0 35rpx;
  background:#f8f8f8;
}

.lingoInfo{
  height:150rpx;
  width: 100%;
  background:white;
  border-radius:15rpx;
  margin-bottom:20rpx;

}
.lingoInfoImg{
  margin:20rpx 0 0 20rpx;
  width: 81rpx;
  height: 81rpx;
}
.lingoText{
  position: absolute;
  margin-left:230rpx;
  margin-top:50rpx;
}
.lingoTime{
  position: absolute;
  margin-top:100rpx;
  margin-left:-90rpx;
  font-size:26rpx;
}
.rate{
  display:flex;
  justify-content:space-between;
  flex-wrap:wrap;
  align-items:center;
}
.outer{
  width:100%;
  height:60rpx;
  background:#fff;
  border-radius:50rpx;
}
.inner{
  margin:15rpx auto;
  width:90%;
  height:30rpx;
  background:#eee;
  border-radius:50rpx;

}