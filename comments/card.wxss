.card {
    display: flex;
    justify-content: center;
    align-content: center;
    text-align: center;
    height: 200rpx;
    width: 700rpx;
    margin: 100rpx 0 0 25rpx;
    border-radius: 20rpx;
    background: rgb(254, 255, 255);
    box-shadow: 0 10px 20px rgba(0, 0, 0, 0.03), 0 6px 6px rgba(0, 0, 0, 0.05);
}